{
    "name": "webinarium/linode-api3",
    "description": "Linode API v3 Client Library for PHP",
    "type": "library",
    "keywords": ["api", "linode"],
    "homepage": "https://github.com/webinarium/linode-api3",
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "friendsofphp/php-cs-fixer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Linode\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
